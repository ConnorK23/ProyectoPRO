package dao;

import java.util.List;

import model.Clientes;

public interface IClientesDAO {
	void filtrarTabla(String campo, String texto);
	boolean actualizar(String np);
	void eliminar(String np);
	void filtrar(String np);
	void llenarTabla();
	boolean validar();
	boolean nuevo(Clientes clientes);
	List<Clientes> getClientes();
	boolean actualizar(Clientes clientes, String np);
}
